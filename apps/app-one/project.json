{
  "projectType": "application",
  "root": "apps/app-one",
  "sourceRoot": "apps/app-one/src",
  "prefix": "mfe-prototype",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "baseHref": "/one/",
        "outputPath": "dist/app-one/browser",
        "index": "apps/app-one/src/index.html",
        "main": "apps/app-one/src/main.ts",
        "polyfills": "apps/app-one/src/polyfills.ts",
        "tsConfig": "apps/app-one/tsconfig.app.json",
        "inlineStyleLanguage": "less",
        "assets": ["apps/app-one/src/favicon.ico", "apps/app-one/src/assets"],
        "styles": ["apps/app-one/src/styles.less"],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/app-one/webpack.config.js"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/app-one/src/environments/environment.ts",
              "with": "apps/app-one/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/app-one/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-server",
      "configurations": {
        "production": {
          "browserTarget": "app-one:build:production"
        },
        "development": {
          "browserTarget": "app-one:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4201,
        "publicHost": "http://localhost:4201"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "app-one:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/app-one/src/**/*.ts",
          "apps/app-one/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/app-one"],
      "options": {
        "jestConfig": "apps/app-one/jest.config.js",
        "passWithNoTests": true
      }
    },
    "server": {
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/app-one/server",
        "main": "apps/app-one/server.ts",
        "tsConfig": "apps/app-one/tsconfig.server.json",
        "inlineStyleLanguage": "less"
      },
      "configurations": {
        "production": {
          "outputHashing": "media",
          "fileReplacements": [
            {
              "replace": "apps/app-one/src/environments/environment.ts",
              "with": "apps/app-one/src/environments/environment.prod.ts"
            }
          ]
        },
        "development": {
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-ssr": {
      "executor": "@nguniversal/builders:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "app-one:build:development",
          "serverTarget": "app-one:server:development"
        },
        "production": {
          "browserTarget": "app-one:build:production",
          "serverTarget": "app-one:server:production"
        }
      },
      "defaultConfiguration": "development"
    },
    "prerender": {
      "executor": "@nguniversal/builders:prerender",
      "options": {
        "routes": [
          "/"
        ]
      },
      "configurations": {
        "production": {
          "browserTarget": "app-one:build:production",
          "serverTarget": "app-one:server:production"
        },
        "development": {
          "browserTarget": "app-one:build:development",
          "serverTarget": "app-one:server:development"
        }
      },
      "defaultConfiguration": "production"
    }
  },
  "tags": []
}
