{
  "projectType": "application",
  "root": "apps/app-one-pug",
  "sourceRoot": "apps/app-one-pug/src",
  "prefix": "mfe-prototype",
  "targets": {
    "build": {
      "executor": "@angular-builders/custom-webpack:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "baseHref": "/one/",
        "outputPath": "dist/apps/app-one-pug",
        "index": "apps/app-one-pug/src/index.html",
        "indexTransform": "tools/pug/index-compile.ts",
        "main": "apps/app-one-pug/src/main.ts",
        "polyfills": "apps/app-one-pug/src/polyfills.ts",
        "tsConfig": "apps/app-one-pug/tsconfig.app.json",
        "inlineStyleLanguage": "less",
        "assets": [
          "apps/app-one-pug/src/favicon.ico",
          "apps/app-one-pug/src/assets"
        ],
        "styles": ["apps/app-one-pug/src/styles.less"],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/app-one-pug/webpack.config"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/app-one-pug/src/environments/environment.ts",
              "with": "apps/app-one-pug/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/app-one-pug/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-builders/custom-webpack:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "app-one-pug:build:production"
        },
        "development": {
          "browserTarget": "app-one-pug:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4301,
        "publicHost": "http://localhost:4301"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "app-one-pug:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/app-one-pug/src/**/*.ts",
          "apps/app-one-pug/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/app-one-pug"],
      "options": {
        "jestConfig": "apps/app-one-pug/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
